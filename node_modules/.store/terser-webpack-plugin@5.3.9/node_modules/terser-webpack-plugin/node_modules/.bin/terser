#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/17354/Desktop/chat-app/node_modules/.store/terser@5.19.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/17354/Desktop/chat-app/node_modules/.store/terser@5.19.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@5.19.1/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@5.19.1/node_modules/terser/bin/terser" "$@"
fi
